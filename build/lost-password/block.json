      "type": "string",
      "default": ""
    },
    "resetButtonLabel": {
      "type": "string",
      "default": "Reset Password"
    },
    "uniqueId": {
      "type": "string",
      "default": ""
    }
  },
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "boosted-front-end-login",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}