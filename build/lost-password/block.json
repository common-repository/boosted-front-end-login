{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "boosted/boosted-lost-password",
  "version": "0.1.0",
  "title": "Lost Password",
  "category": "boosted-front-end-login",
  "icon": "smiley",
  "description": "Lost password form",
  "attributes": {
    "lostDescription": {
      "type": "string",
